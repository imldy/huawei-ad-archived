function d(s,v,w,u){$(".s-wrap-playskill .s-main .content .page-ctrl").empty().css("display","none");
this.currPage=s;
var r={product_code:p,siteID:window.digitalData.page.pageInfo.siteCode2.toUpperCase(),pageNo:s.toString(),pageSize:m.toString(),qAppName:"ServiceOfficial",knowledge_type:"1",q:b,language:getSiteLanguage()};
