d.prototype={constructor:d,reformatDate:function(B,s){var n=this,x=typeof s==="undefined"?n.options.format:s,m=(x.match(/d/g)||[]).length,r=(x.match(/m/g)||[]).length,A=(x.match(/y/g)||[]).length,y=r==4,l=r==1,v=m==1,z=B.length,u=x.indexOf("m"),C=x.indexOf("d"),o=x.indexOf("y"),D="",w="",p="";
