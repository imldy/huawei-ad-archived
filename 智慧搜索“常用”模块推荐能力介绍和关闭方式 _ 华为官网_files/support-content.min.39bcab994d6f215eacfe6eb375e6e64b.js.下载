(function(b,a){if(!b){return a
}var c=function(){this.el=a;
this.items=a;
this.sizes=[];
this.max=[0,0];
this.current=0;
this.interval=a;
this.opts={speed:500,delay:3000,complete:a,keys:!a,dots:a,fluid:a};
var d=this;
this.init=function(e,f){this.el=e;
this.ul=e.children("ul");
this.max=[e.outerWidth(),e.outerHeight()];
this.items=this.ul.children("li").each(this.calculate);
this.opts=b.extend(this.opts,f);
this.setup();
return this
};
this.calculate=function(f){var h=b(this),e=h.outerWidth(),g=h.outerHeight();
d.sizes[f]=[e,g];
if(e>d.max[0]){d.max[0]=e
}if(g>d.max[1]){d.max[1]=g
}};
this.setup=function(){this.el.css({overflow:"hidden",width:d.max[0],height:this.items.first().outerHeight()});
this.ul.css({width:this.items.length*100+"%",position:"relative"});
this.items.css("width",100/this.items.length+"%");
if(this.opts.delay!==a){this.start();
this.el.hover(this.stop,this.start)
}this.opts.keys&&b(document).keydown(this.keys);
this.opts.dots&&this.dots();
if(this.opts.fluid){var e=function(){d.el.css("width",Math.min(Math.round(d.el.outerWidth()/d.el.parent().outerWidth()*100),100)+"%")
};
e();
b(window).resize(e)
}if(this.opts.arrows){this.el.parent().append('<p class="arrows"><span class="prev">←</span><span class="next">→</span></p>').find(".arrows span").click(function(){b.isFunction(d[this.className])&&d[this.className]()
})
}if(b.event.swipe){this.el.on("swipeleft",d.prev).on("swiperight",d.next)
}};
this.move=function(f,h){if(!this.items.eq(f).length){f=0
}if(f<0){f=this.items.length-1
}var e=this.items.eq(f);
var g={height:e.outerHeight()};
var j=h?5:this.opts.speed;
if(!this.ul.is(":animated")){d.el.find(".dot:eq("+f+")").addClass("active").siblings().removeClass("active");
this.el.animate(g,j)&&this.ul.animate(b.extend({left:"-"+f+"00%"},g),j,function(k){d.current=f;
b.isFunction(d.opts.complete)&&!h&&d.opts.complete(d.el)
})
}};
this.start=function(){d.interval=setInterval(function(){d.move(d.current+1)
},d.opts.delay)
};
this.stop=function(){d.interval=clearInterval(d.interval);
return d
};
this.keys=function(f){var g=f.which;
var e={37:d.prev,39:d.next,27:d.stop};
if(b.isFunction(e[g])){e[g]()
}};
this.next=function(){return d.stop().move(d.current+1)
};
this.prev=function(){return d.stop().move(d.current-1)
};
this.dots=function(){var e='<ol class="dots">';
b.each(this.items,function(f){e+='<li class="dot'+(f<1?" active":"")+'">'+(f+1)+"</li>"
});
e+="</ol>";
this.el.addClass("has-dots").append(e).find(".dot").click(function(){d.move(b(this).index())
})
}
};
b.fn.unslider=function(d){var e=this.length;
return this.each(function(g){var h=b(this);
var f=(new c).init(h,d);
h.data("unslider"+(e>1?"-"+(g+1):""),f)
})
}
