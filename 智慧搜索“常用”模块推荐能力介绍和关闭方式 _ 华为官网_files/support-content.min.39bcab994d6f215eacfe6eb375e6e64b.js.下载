$(document).ready(function(){if($(".psirt").length>0){var a=3;
var b=$(window).width()>=768?20:10;
d();
$(".list-content .versions").each(function(){var k=$(this);
var g=k.find(".p-des1").text().split(";");
var j=k.find("div").empty();
var f=g.length;
for(var h=0;
h<f;
h++){j.append("<p class='p-des1'>"+$.trim(g[h])+"</p>")
}});
$(".psirt .p-list").on("click",".li-title",function(){var g=$(this).hasClass("slidedown")?"slidedown":"slideup";
var h=$(this);
h.parent().siblings().removeClass("active");
h.parent().toggleClass("active");
var f=h.parent().siblings();
f.find(".list").slideUp();
f.find(".li-title").removeClass("slideup").addClass("slidedown");
f.find(".versions .p-tit").removeClass("slideup").addClass("slidedown");
f.find(".versions .p-tit").removeClass("slideup").addClass("slidedown").siblings("div").hide();
if(g=="slideup"){h.removeClass("slideup").addClass("slidedown");
h.siblings(".list").slideUp().removeClass("show").addClass("hide")
}else{if(g=="slidedown"){h.removeClass("slidedown").addClass("slideup");
h.siblings(".list").slideDown().removeClass("hide").addClass("show")
}}setTimeout(function(){var i=$(window).width()>=768?parseInt($("#header-v3").height()):parseInt($(".app-nav").height());
var j=parseInt(h.offset().top)-i;
$(document).scrollTop(j)
},500)
}).on("click",".versions .p-tit",function(){var f=$(this).hasClass("slidedown")?"slidedown":"slideup";
if(f=="slideup"){$(this).removeClass("slideup").addClass("slidedown")
}else{if(f=="slidedown"){$(this).removeClass("slidedown").addClass("slideup")
}}$(this).siblings("div").stop().slideToggle()
});
$(".psirt .years").on("click",".y-year i",function(){var g=$(this).attr("year");
var f=$(".list-content.show").attr("year");
if(g!=f){$(".psirt .page-count-wrap").hide();
$(this).addClass("active").siblings().removeClass("active");
$(".list-content.show").removeClass("show").addClass("hide");
$(".list-content[year='"+g+"']").removeClass("hide").addClass("show");
d();
e($(".list-content[year='"+f+"']").find("li"))
}}).on("click",".y-prev",function(){if(!$(this).hasClass("disable")){var h=$(".y-year i");
var j=$(".y-year i.show");
var f=h.index(j[0]);
j.removeClass().addClass("hide");
for(var g=1;
g<=a;
g++){if(g==a){h.eq(f-g).removeClass("hide").addClass("show").addClass("active");
h.eq(f-g).click()
}else{h.eq(f-g).removeClass("hide").addClass("show")
}}(f-a)<=0?$(this).addClass("disable"):"";
$(this).siblings(".y-next").removeClass("disable")
}}).on("click",".y-next",function(){if(!$(this).hasClass("disable")){var h=$(".y-year i");
var j=$(".y-year i.show");
var f=h.index(j[a-1]);
j.removeClass().addClass("hide");
for(var g=1;
g<=a;
g++){if(g==1){h.eq(f+g).removeClass("hide").addClass("show").addClass("active");
h.eq(f+g).click()
}else{h.eq(f+g).removeClass("hide").addClass("show")
}}(f+a+1)>=h.length?$(this).addClass("disable"):"";
$(this).siblings(".y-prev").removeClass("disable")
}});
$(".psirt .page-count-wrap").on("click",".prev-page",function(){if(!$(this).hasClass("icon-disabled")){var f=$(".list-content.show li");
var k=$(".list-content.show").find("li.show");
var h=f.index(k.first());
c(k);
for(var j=1;
j<=b;
j++){f.eq(h-j).removeClass("hide").addClass("show")
}var g=$(".psirt .page-count-wrap");
var l=parseInt(g.find(".current-page").text())-1;
g.find(".current-page").text(l);
1==l?g.find(".prev-page").addClass("icon-disabled"):"";
g.find(".next-page").removeClass("icon-disabled")
}}).on("click",".next-page",function(){if(!$(this).hasClass("icon-disabled")){var f=$(".list-content.show li");
var k=$(".list-content.show").find("li.show");
var h=f.index(k.last());
c(k);
for(var j=1;
j<=b;
j++){f.eq(j+h).removeClass("hide").addClass("show")
}var g=$(".psirt .page-count-wrap");
