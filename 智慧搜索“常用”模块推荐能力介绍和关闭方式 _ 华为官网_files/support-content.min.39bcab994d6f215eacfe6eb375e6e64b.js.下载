g.find(".current-page").text(l);
g.find(".total-page").text()==l?g.find(".next-page").addClass("icon-disabled"):"";
g.find(".prev-page").removeClass("icon-disabled")
}});
function d(){var j=$(".list-content.show li");
var f=j.length;
for(var h=0;
h<f;
h++){var k=j.eq(h);
h<b?k.removeClass("hide").addClass("show"):k.removeClass("show").addClass("hide");
k.removeClass("active").find(".li-title").removeClass("slideup").addClass("slidedown");
k.find(".list").removeClass("show").addClass("hide");
k.find(".versions .p-tit").removeClass("slideup").addClass("slidedown");
k.find(".versions div").removeClass("show").addClass("hide")
}var g=$(".psirt .page-count-wrap");
g.find(".current-page").text(1);
g.find(".total-page").text(Math.ceil(f/b));
g.find(".prev-page").addClass("icon-disabled");
g.find(".next-page").addClass("icon-disabled");
if(f>b){g.find(".next-page").removeClass("icon-disabled")
}g.show()
}function e(h){var f=h.length;
for(var g=0;
g<f;
g++){var j=h.eq(g);
g<b?j.removeClass("hide").addClass("show"):j.removeClass("show").addClass("hide");
j.removeClass("active").find(".li-title").removeClass("slideup").addClass("slidedown");
j.find(".list").removeClass("show").addClass("hide").hide();
j.find(".versions .p-tit").removeClass("slideup").addClass("slidedown");
j.find(".versions div").hide()
}}function c(g){var f=$(window).width()>=768?parseInt($("#header-v3").height()):parseInt($(".app-nav").height());
var h=parseInt($(".psirt .years").offset().top)-f;
$(document).scrollTop(h);
g.each(function(){$(this).removeClass("show active").addClass("hide");
$(this).find(".li-title").removeClass("slideup").addClass("slidedown");
$(this).find(".list").removeClass("show").addClass("hide").hide();
$(this).find(".versions>p-tit").removeClass("slideup").addClass("slidedown");
$(this).find(".versions>div").hide()
})
}}});
