case"form":return("function"==typeof this.reset||"object"==typeof this.reset&&!this.reset.nodeType)&&this.reset(),!0;
default:return t.find("form,input,label,select,textarea").resetForm(),!0
}})
},e.fn.enable=function(e){return void 0===e&&(e=!0),this.each(function(){this.disabled=!e
})
},e.fn.selected=function(t){return void 0===t&&(t=!0),this.each(function(){var r=this.type;
