}return{m:D,d:w,y:p,date:isNaN(parseInt(D))?new Date(D+" "+w+", "+p):new Date(p,D-1,w)}
},formatDate:function(k){var l=new Date(this.selected),j=l.getDate(),i=l.getMonth(),n=l.getFullYear();
return k.replace(/(yyyy|yy|mmmm|mmm|mm|m|dd|d)/gi,function(m){switch(m.toLowerCase()){case"d":return j;
case"dd":return(j<10?"0"+j:j);
case"m":return i+1;
case"mm":return(i+1<10?"0"+(i+1):(i+1));
case"mmm":return b[i];
case"mmmm":return a[i];
case"yy":return n.toString().substr(2,2);
case"yyyy":return n
}})
