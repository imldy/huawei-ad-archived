f.fn.dcalendarpicker=function(i){return f(this).each(function(){var j=f(this);
if(i){i.mode="datepicker";
j.dcalendar(i)
}else{j.dcalendar({mode:"datepicker"})
}j.on("click",function(l){var k=j.data(c);
k.show();
this.blur()
}).on("dateselected",function(l){var k=j.data(c);
j.val(l.date).trigger("onchange");
k.hide(function(){j.trigger(f.Event("datechanged",{date:l.date}))
})
}).on("keydown",function(k){if(e.indexOf(k.which)<0&&j.data(c).options.readOnly){return false
}});
f(document).on("keydown",function(k){if(k.keyCode!=27){return
}j.data(c).hide()
})
})
