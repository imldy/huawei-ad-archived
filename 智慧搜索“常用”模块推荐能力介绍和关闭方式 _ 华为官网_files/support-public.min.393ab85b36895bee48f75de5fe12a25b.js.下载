N.prototype.selectMany=function(g,d){var k=this;
g=M.createLambda(g);
null==d&&(d=function(p,m){return m
});
d=M.createLambda(d);
return new N(function(){var r,p=I,m=0;
return new L(function(){r=k.getEnumerator()
},function(){if(p===I&&!r.moveNext()){return !1
}do{if(null==p){var s=g(r.current(),m++);
p=N.from(s).getEnumerator()
}if(p.moveNext()){return this.yieldReturn(d(r.current(),p.current()))
}M.dispose(p);
p=null
}while(r.moveNext());
return !1
},function(){try{M.dispose(r)
}finally{M.dispose(p)
}})
})
};
