},function(){for(var w;
!0==(w=v.moveNext());
){if(t===p(k(v.current()))){s.push(d(v.current()))
}else{break
}}if(0<s.length){var x=g?r(u,N.from(s)):r(u,s);
w?(u=k(v.current()),t=p(u),s=[d(v.current())]):s=[];
