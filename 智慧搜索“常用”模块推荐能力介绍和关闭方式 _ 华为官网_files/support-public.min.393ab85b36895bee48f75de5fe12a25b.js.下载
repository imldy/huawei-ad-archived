var bdMap=(function(){function a(b){console.log("bdMap");
this.eleId=b;
this.iconUrl=sysConfig.mapIcoUrl;
this.baiduMap={};
this.localSearch={};
this.__marker__=[];
this.__marker_click__;
this.__city__;
this.__local_city_info__
}a.prototype.getMap=function(){return this.baiduMap
};
a.prototype.init=function(){this.baiduMap=new BMap.Map(this.eleId);
this.baiduMap.enableScrollWheelZoom();
this.baiduMap.enableContinuousZoom();
this.baiduMap.centerAndZoom("北京市",15);
var c={anchor:BMAP_ANCHOR_TOP_RIGHT};
this.baiduMap.addControl(new BMap.NavigationControl(c));
try{var b=this;
this.__city__=new BMap.LocalCity();
this.__city__.get(function(f){b.__local_city_info__=f
})
}catch(d){}console.log("bdMap init")
};
a.prototype.getPoints=function(b){return this.__marker__
};
a.prototype.clearMarkers=function(){this.baiduMap.clearOverlays()
};
a.prototype.addMarkers=function(o,c){var h=this;
this.__marker__=[];
var l=this.__marker__;
for(var i=0;
i<o.length;
i++){var p=o[i];
var n=new BMap.Point(p.x,p.y);
var j=new BMap.Size(30,40);
var k=new BMap.Icon(this.iconUrl.replace("{index}",i+1),j);
if(c&&c!=""){if(c.indexOf("huawei-cbg-site")>0){var d=c.substring(0,c.length-1)+".html/"+p.shopCode.toLocaleLowerCase()+".html?wcmmode=disabled"
}else{var d=c+p.shopCode.toLocaleLowerCase()+"/"
}}else{var m=window.location.href;
if(m.indexOf("content/huawei-cbg-site")>0){var d=m.replace(".html","/detail.html/"+p.shopCode.toLocaleLowerCase()+".html")
}else{var d=m+"detail/"+p.shopCode.toLocaleLowerCase()+"/"
}}var b="<div class='info_baidu' style='padding-top:5px;'>   <div class='info_baidu_main' style='max-width:300px;'>       <p class='title'>"+p.shopName+"</p>       <p class='address'>"+p.shopAddress+"</p>       <p class='tel'>"+p.shopTel+"</p>   	<p class='shop-detail'><a onclick='sendDmpaByCurrentTag(this);gaAndAaByCurrentTag(this);haDispachByTag(this)' ha-id='service_center_click_inquire_service_center' cat='service center' act='Click on service center detail' lab='inquire service center' target='_blank' href='"+d+"'>门店详情</a></p>   </div></div>";
var g=new BMap.Marker(n,{icon:k});
this.baiduMap.addOverlay(g);
var f=new BMap.InfoWindow(b);
(function(r,t,q,s){s.push({x:q.x,y:q.y,mark:t,infoWindow:r});
t.addEventListener("click",function(){this.openInfoWindow(r);
h.__marker_click__&&h.__marker_click__(q)
})
})(f,g,p,l);
if(i==0){this.baiduMap.centerAndZoom(n,13)
}}};
a.prototype.getLocation=function(b,f){var b=new BMap.Point(b.x,b.y);
var c=new BMap.Geocoder();
var d=this;
c.getLocation(b,function(g){var h=g.addressComponents;
if(h.province==""&&h.city==""&&h.district==""&&h.street==""&&h.streetNumber==""){}f&&f(h)
})
};
a.prototype.getCity=function(){return this.__local_city_info__
};
a.prototype.getPoint=function(b,c){this.localSearch=new BMap.LocalSearch(this.baiduMap);
this.localSearch.setSearchCompleteCallback(function(d){try{var g=d.getPoi(0);
c&&c({x:g.point.lng,y:g.point.lat})
}catch(f){c&&c(null);
console.log(f)
}});
this.localSearch.search(b)
};
a.prototype.MarkClick=function(b){this.__marker_click__=b
};
return a
})();
