return this.yieldReturn(r)
}if(0>=p.length){return !1
}M.dispose(m);
m=p.pop()
}},function(){try{M.dispose(m)
}finally{N.from(p).forEach(function(r){r.dispose()
})
}})
})
};
N.prototype.flatten=function(){var d=this;
return new N(function(){var g,k=null;
return new L(function(){g=d.getEnumerator()
},function(){for(;
;
){if(null!=k){if(k.moveNext()){return this.yieldReturn(k.current())
}k=null
}if(g.moveNext()){if(g.current() instanceof Array){M.dispose(k);
