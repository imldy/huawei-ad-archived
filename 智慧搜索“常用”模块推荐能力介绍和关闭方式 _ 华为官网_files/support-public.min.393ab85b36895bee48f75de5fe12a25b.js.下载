};
N.prototype.concat=function(){var g=this;
if(1==arguments.length){var d=arguments[0];
return new N(function(){var p,m;
return new L(function(){p=g.getEnumerator()
},function(){if(null==m){if(p.moveNext()){return this.yieldReturn(p.current())
