N.prototype.scan=function(g,d){var m;
null==d?(d=M.createLambda(g),m=!1):(d=M.createLambda(d),m=!0);
var k=this;
return new N(function(){var r,p,s=!0;
return new L(function(){r=k.getEnumerator()
},function(){if(s){s=!1;
if(m){return this.yieldReturn(p=g)
}if(r.moveNext()){return this.yieldReturn(p=r.current())
}}return r.moveNext()?this.yieldReturn(p=d(p,r.current())):!1
},function(){M.dispose(r)
})
})
};
N.prototype.select=function(g){g=M.createLambda(g);
