}else{d=$("#LAG_INQUIRY_BILL_DOOR_SERVICE").val()
}break
}return d
}function currentSixMonthAgo(c){var g=new Date();
var f=g.getFullYear();
var h=g.getMonth()+1;
var b=g.getDate();
var d=null;
if(c=="sixMonthAgo"){if(h<=6){f=f-1;
h=h+6
}else{h=h-6
}}if(h<10){h="0"+h
}var a=new Date(f,h,0).getDate();
if(b>a){b=a
}d=f+"-"+h+"-"+b;
return d
