return new N(function(){var v,u,t=null,s=0;
return new L(function(){v=g.getEnumerator();
u=N.from(k).toLookup(r,F,m)
},function(){for(;
;
){if(null!=t){var w=t[s++];
if(w!==I){return this.yieldReturn(p(v.current(),w))
}s=0
}if(v.moveNext()){w=d(v.current()),t=u.get(w).toArray()
}else{return !1
}}},function(){M.dispose(v)
})
})
};
