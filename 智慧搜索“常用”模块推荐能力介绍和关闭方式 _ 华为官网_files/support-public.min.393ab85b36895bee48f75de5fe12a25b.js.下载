N.prototype.choose=function(g){g=M.createLambda(g);
var d=this;
return new N(function(){var m,k=0;
return new L(function(){m=d.getEnumerator()
},function(){for(;
m.moveNext();
){var p=g(m.current(),k++);
if(null!=p){return this.yieldReturn(p)
}}return this.yieldBreak()
},function(){M.dispose(m)
})
})
};
N.prototype.ofType=function(g){var d;
switch(g){case Number:d="number";
break;
case String:d="string";
break;
case Boolean:d="boolean";
break;
case Function:d="function";
break;
default:d=null
}return null===d?this.where(function(k){return k instanceof g
}):this.where(function(k){return typeof k===d
})
};
N.prototype.zip=function(){var g=arguments,d=M.createLambda(arguments[arguments.length-1]),m=this;
if(2==arguments.length){var k=arguments[0];
return new N(function(){var r,p,s=0;
return new L(function(){r=m.getEnumerator();
p=N.from(k).getEnumerator()
},function(){return r.moveNext()&&p.moveNext()?this.yieldReturn(d(r.current(),p.current(),s++)):!1
},function(){try{M.dispose(r)
}finally{M.dispose(p)
}})
})
}return new N(function(){var r,p=0;
return new L(function(){var s=N.make(m).concat(N.from(g).takeExceptLast().select(N.from)).select(function(t){return t.getEnumerator()
}).toArray();
r=N.from(s)
},function(){if(r.all(function(t){return t.moveNext()
})){var s=r.select(function(t){return t.current()
}).toArray();
s.push(p++);
return this.yieldReturn(d.apply(null,s))
}return this.yieldBreak()
},function(){N.from(r).forEach(M.dispose)
})
})
};
N.prototype.merge=function(){var g=arguments,d=this;
return new N(function(){var m,k=-1;
return new L(function(){m=N.make(d).concat(N.from(g).select(N.from)).select(function(p){return p.getEnumerator()
}).toArray()
},function(){for(;
0<m.length;
){k=k>=m.length-1?0:k+1;
var p=m[k];
if(p.moveNext()){return this.yieldReturn(p.current())
}p.dispose();
m.splice(k--,1)
}return this.yieldBreak()
},function(){N.from(m).forEach(M.dispose)
})
})
};
N.prototype.join=function(k,d,r,p,m){d=M.createLambda(d);
r=M.createLambda(r);
p=M.createLambda(p);
m=M.createLambda(m);
