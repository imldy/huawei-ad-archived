},function(){for(;
r.moveNext();
){var s=r.current();
if(!m.contains(s)&&p.contains(s)){return m.add(s),this.yieldReturn(s)
}}return !1
},function(){M.dispose(r)
})
})
};
N.prototype.sequenceEqual=function(g,d){d=M.createLambda(d);
var m=this.getEnumerator();
try{var k=N.from(g).getEnumerator();
try{for(;
m.moveNext();
){if(!k.moveNext()||d(m.current())!==d(k.current())){return !1
}}return k.moveNext()?!1:!0
}finally{M.dispose(k)
}}finally{M.dispose(m)
}};
N.prototype.union=function(g,d){d=M.createLambda(d);
var k=this;
return new N(function(){var r,p,m;
return new L(function(){r=k.getEnumerator();
m=new h(d)
},function(){var s;
