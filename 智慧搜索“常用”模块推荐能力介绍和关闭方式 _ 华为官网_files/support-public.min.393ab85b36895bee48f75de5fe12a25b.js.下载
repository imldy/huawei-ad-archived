return new L(function(){g=d.toArray();
k=g.length
},function(){return 0<k?this.yieldReturn(g[--k]):!1
},K)
})
};
N.prototype.shuffle=function(){var d=this;
return new N(function(){var g;
return new L(function(){g=d.toArray()
},function(){if(0<g.length){var k=Math.floor(Math.random()*g.length);
return this.yieldReturn(g.splice(k,1)[0])
}return !1
},K)
})
};
N.prototype.weightedSample=function(g){g=M.createLambda(g);
var d=this;
return new N(function(){var m,k=0;
return new L(function(){m=d.choose(function(p){var r=g(p);
if(0>=r){return null
}k+=r;
return{value:p,bound:k}
}).toArray()
},function(){if(0<m.length){for(var r=Math.floor(Math.random()*k)+1,p=-1,t=m.length;
1<t-p;
){var s=Math.floor((p+t)/2);
m[s].bound>=r?t=s:p=s
}return this.yieldReturn(m[t].value)
}return this.yieldBreak()
},K)
})
};
N.prototype.groupBy=function(g,d,p,m){var k=this;
g=M.createLambda(g);
d=M.createLambda(d);
null!=p&&(p=M.createLambda(p));
m=M.createLambda(m);
return new N(function(){var r;
return new L(function(){r=k.toLookup(g,d,m).toEnumerable().getEnumerator()
},function(){for(;
r.moveNext();
){return null==p?this.yieldReturn(r.current()):this.yieldReturn(p(r.current().key(),r.current()))
}return !1
},function(){M.dispose(r)
})
})
};
N.prototype.partitionBy=function(k,d,r,p){var m=this;
k=M.createLambda(k);
d=M.createLambda(d);
p=M.createLambda(p);
