r.moveNext();
){if(s=r.current(),!m.contains(s)){return m.add(s),this.yieldReturn(s)
}}p=N.from(g).getEnumerator()
}for(;
p.moveNext();
){if(s=p.current(),!m.contains(s)){return m.add(s),this.yieldReturn(s)
}}return !1
},function(){try{M.dispose(r)
}finally{M.dispose(p)
}})
})
};
