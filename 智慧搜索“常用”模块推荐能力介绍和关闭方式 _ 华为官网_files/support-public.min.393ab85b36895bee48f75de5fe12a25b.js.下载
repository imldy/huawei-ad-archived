N.prototype.insert=function(g,d){var k=this;
return new N(function(){var r,p,m=0,s=!1;
return new L(function(){r=k.getEnumerator();
p=N.from(d).getEnumerator()
},function(){return m==g&&p.moveNext()?(s=!0,this.yieldReturn(p.current())):r.moveNext()?(m++,this.yieldReturn(r.current())):!s&&p.moveNext()?this.yieldReturn(p.current()):!1
},function(){try{M.dispose(r)
}finally{M.dispose(p)
}})
})
};
N.prototype.alternate=function(g){var d=this;
return new N(function(){var r,p,m,k;
return new L(function(){m=g instanceof Array||null!=g.getEnumerator?N.from(N.from(g).toArray()):N.make(g);
