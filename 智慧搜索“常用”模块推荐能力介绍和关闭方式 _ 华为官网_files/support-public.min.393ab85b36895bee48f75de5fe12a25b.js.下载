}else{return !1
}}},function(){M.dispose(r)
})
})
};
N.prototype.traverseDepthFirst=function(g,d){var k=this;
g=M.createLambda(g);
d=M.createLambda(d);
return new N(function(){var p=[],m;
return new L(function(){m=k.getEnumerator()
},function(){for(;
;
){if(m.moveNext()){var r=d(m.current(),p.length);
p.push(m);
