var useNewInterFace=false;
if(apiurl.indexOf("https://ccpce-sit.cbgit.huawei.com/")>=0||apiurl.indexOf("https://ccpce-cn.consumer.huawei.com")>=0||apiurl.indexOf("ccpce-icsl.test.huawei.com")>=0){useNewInterFace=true
}function objectClone(c){var b;
if(c.constructor==Object){b=new c.constructor()
}else{b=new c.constructor(c.valueOf())
}for(var a in c){if(b[a]!=c[a]){if(typeof(c[a])=="object"){b[a]=objectClone(c[a])
}else{b[a]=c[a]
}}}b.toString=c.toString;
b.valueOf=c.valueOf;
return b
}function buildRequestObject(a){var d=objectClone(a);
if(!d.channelCode){d.channelCode=requestConfig.channelCode
}if(!d.countryCode){d.countryCode=requestConfig.countryCode
}if(!d.langCode){d.langCode=requestConfig.langCode
}if(!d.country){d.country=requestConfig.country
}if(!d.language){d.language=requestConfig.language
}if(!d.siteCode){d.siteCode=requestConfig.siteCode
}var c={};
for(var b in d){if(b!="toString"&&b!="valueOf"){c[b]=d[b]
}}return c
}function buildCCPDRequestObject(a){var d=objectClone(a);
if(!d.channelCode){d.channelCode=requestConfig.channelCode
}if(!d.countryCode){d.countryCode=requestConfig.countryCode
}d.langCode=(!requestConfig.ccpdLangCode)?requestConfig.langCode:requestConfig.ccpdLangCode;
if(!d.country){d.country=requestConfig.country
}d.language=(!requestConfig.ccpdLangCode)?requestConfig.language:requestConfig.ccpdLangCode;
if(!d.siteCode){d.siteCode=requestConfig.siteCode
}var c={};
for(var b in d){if(b!="toString"&&b!="valueOf"){c[b]=d[b]
}}return c
