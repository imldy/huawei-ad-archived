a=a.replace(/<bgsound\b[^<]*(?:(?!<\/bgsound>)<[^<]*)*<\/bgsound>?/gi,"");
a=a.replace(/<blink\b[^<]*(?:(?!<\/blink>)<[^<]*)*<\/blink>?/gi,"");
a=a.replace(/<body\b[^<]*(?:(?!<\/body>)<[^<]*)*<\/body>?/gi,"");
a=a.replace(/<embed\b[^<]*(?:(?!<\/embed>)<[^<]*)*<\/embed>?/gi,"");
a=a.replace(/<frame\b[^<]*(?:(?!<\/frame>)<[^<]*)*<\/frame>?/gi,"");
a=a.replace(/<frameset\b[^<]*(?:(?!<\/frameset>)<[^<]*)*<\/frameset>?/gi,"");
a=a.replace(/<head\b[^<]*(?:(?!<\/head>)<[^<]*)*<\/head>?/gi,"");
a=a.replace(/<html\b[^<]*(?:(?!<\/html>)<[^<]*)*<\/html>?/gi,"");
a=a.replace(/<ilayer\b[^<]*(?:(?!<\/ilayer>)<[^<]*)*<\/ilayer>?/gi,"");
a=a.replace(/<layer\b[^<]*(?:(?!<\/layer>)<[^<]*)*<\/layer>?/gi,"");
a=a.replace(/<link\b[^<]*(?:(?!<\/link>)<[^<]*)*<\/link>?/gi,"");
a=a.replace(/<meta\b[^<]*(?:(?!<\/meta>)<[^<]*)*<\/meta>?/gi,"");
a=a.replace(/<object\b[^<]*(?:(?!<\/object>)<[^<]*)*<\/object>?/gi,"");
a=a.replace(/<style\b[^<]*(?:(?!<\/style>)<[^<]*)*<\/style>?/gi,"");
a=a.replace(/<title\b[^<]*(?:(?!<\/title>)<[^<]*)*<\/title>?/gi,"");
a=a.replace(/<script.*?>/ig,"");
