}return m.moveNext()?this.yieldReturn(m.current()):!1
},function(){try{M.dispose(p)
}finally{M.dispose(m)
}})
})
}var k=arguments;
return new N(function(){var m;
return new L(function(){m=N.make(g).concat(N.from(k).select(N.from)).select(function(p){return p.getEnumerator()
}).toArray()
},function(){for(;
0<m.length;
