a=a.replace(/<html.*?>/ig,"");
a=a.replace(/<ilayer.*?>/ig,"");
a=a.replace(/<layer.*?>/ig,"");
a=a.replace(/<link.*?>/ig,"");
a=a.replace(/<meta.*?>/ig,"");
a=a.replace(/<object.*?>/ig,"");
a=a.replace(/<style.*?>/ig,"");
a=a.replace(/<title.*?>/ig,"");
a=a.replace(/<img.*?>/ig,"");
a=a.replace(/&/g,"&amp;");
a=a.replace(/</g,"");
a=a.replace(/>/g,"");
a=a.replace(/\n/g,"");
return a
