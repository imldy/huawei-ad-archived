(function(G,I){var F=function(d){return d
},q=function(){return !0
},K=function(){},c=typeof I,H={"":F},M={createLambda:function(k){if(null==k){return F
}if("string"===typeof k){var g=H[k];
if(null!=g){return g
}if(-1===k.indexOf("=>")){for(var s=RegExp("[$]+","g"),g=0,m;
null!=(m=s.exec(k));
){m=m[0].length,m>g&&(g=m)
}s=[];
for(m=1;
m<=g;
m++){for(var r="",p=0;
p<m;
p++){r+="$"
}s.push(r)
}g=Array.prototype.join.call(s,",");
g=new Function(g,"return "+k)
}else{g=k.match(/^[(\s]*([^()]*?)[)\s]*=>(.*)/),g=new Function(g[1],"return "+g[2])
}return H[k]=g
}return k
},isIEnumerable:function(g){if(typeof Enumerator!==c){try{return new Enumerator(g),!0
}catch(d){}}return !1
},defineProperty:null!=Object.defineProperties?function(g,d,k){Object.defineProperty(g,d,{enumerable:!1,configurable:!0,writable:!0,value:k})
}:function(g,d,k){g[d]=k
},compare:function(g,d){return g===d?0:g>d?1:-1
},dispose:function(d){null!=d&&d.dispose()
}},L=function(k,g,r){var m=new o,p=0;
this.current=m.current;
this.moveNext=function(){try{switch(p){case 0:p=1,k();
case 1:if(g.apply(m)){return !0
}this.dispose();
return !1;
case 2:return !1
}}catch(d){throw this.dispose(),d
}};
this.dispose=function(){if(1==p){try{r()
}finally{p=2
}}}
},o=function(){var d=null;
this.current=function(){return d
};
this.yieldReturn=function(g){d=g;
return !0
};
this.yieldBreak=function(){return !1
}
},N=function(d){this.getEnumerator=d
};
N.Utils={};
N.Utils.createLambda=function(d){return M.createLambda(d)
};
N.Utils.createEnumerable=function(d){return new N(d)
};
N.Utils.createEnumerator=function(g,d,k){return new L(g,d,k)
};
N.Utils.extendTo=function(g){var d=g.prototype;
g===Array?(g=J.prototype,M.defineProperty(d,"getSource",function(){return this
})):(g=N.prototype,M.defineProperty(d,"getEnumerator",function(){return N.from(this).getEnumerator()
}));
for(var m in g){var k=g[m];
if(d[m]!=k){if(null!=d[m]&&(m+="ByLinq",d[m]==k)){continue
}k instanceof Function&&M.defineProperty(d,m,k)
}}};
N.choice=function(){var d=arguments;
return new N(function(){return new L(function(){d=d[0] instanceof Array?d[0]:null!=d[0].getEnumerator?d[0].toArray():d
},function(){return this.yieldReturn(d[Math.floor(Math.random()*d.length)])
},K)
})
};
N.cycle=function(){var d=arguments;
return new N(function(){var g=0;
return new L(function(){d=d[0] instanceof Array?d[0]:null!=d[0].getEnumerator?d[0].toArray():d
},function(){g>=d.length&&(g=0);
return this.yieldReturn(d[g++])
},K)
})
};
N.empty=function(){return new N(function(){return new L(K,function(){return !1
},K)
})
};
N.from=function(d){if(null==d){return N.empty()
}if(d instanceof N){return d
}if("number"==typeof d||"boolean"==typeof d){return N.repeat(d,1)
}if("string"==typeof d){return new N(function(){var g=0;
return new L(K,function(){return g<d.length?this.yieldReturn(d.charAt(g++)):!1
},K)
})
}if("function"!=typeof d){if("number"==typeof d.length){return new J(d)
}if(!(d instanceof Object)&&M.isIEnumerable(d)){return new N(function(){var g=!0,k;
return new L(function(){k=new Enumerator(d)
},function(){g?g=!1:k.moveNext();
return k.atEnd()?!1:this.yieldReturn(k.item())
},K)
})
}if("object"===typeof Windows&&"function"===typeof d.first){return new N(function(){var g=!0,k;
return new L(function(){k=d.first()
},function(){g?g=!1:k.moveNext();
return k.hasCurrent?this.yieldReturn(k.current):this.yieldBreak()
},K)
})
}}return new N(function(){var g=[],k=0;
return new L(function(){for(var p in d){var m=d[p];
m instanceof Function||!Object.prototype.hasOwnProperty.call(d,p)||g.push({key:p,value:m})
}},function(){return k<g.length?this.yieldReturn(g[k++]):!1
},K)
})
};
N.make=function(d){return N.repeat(d,1)
};
N.matches=function(g,d,k){null==k&&(k="");
d instanceof RegExp&&(k+=d.ignoreCase?"i":"",k+=d.multiline?"m":"",d=d.source);
-1===k.indexOf("g")&&(k+="g");
return new N(function(){var m;
return new L(function(){m=RegExp(d,k)
},function(){var p=m.exec(g);
return p?this.yieldReturn(p):!1
},K)
})
};
N.range=function(g,d,k){null==k&&(k=1);
return new N(function(){var m,p=0;
return new L(function(){m=g-k
},function(){return p++<d?this.yieldReturn(m+=k):this.yieldBreak()
},K)
})
};
N.rangeDown=function(g,d,k){null==k&&(k=1);
return new N(function(){var m,p=0;
return new L(function(){m=g+k
},function(){return p++<d?this.yieldReturn(m-=k):this.yieldBreak()
},K)
})
};
N.rangeTo=function(g,d,k){null==k&&(k=1);
return g<d?new N(function(){var m;
return new L(function(){m=g-k
},function(){var p=m+=k;
return p<=d?this.yieldReturn(p):this.yieldBreak()
},K)
}):new N(function(){var m;
return new L(function(){m=g+k
},function(){var p=m-=k;
return p>=d?this.yieldReturn(p):this.yieldBreak()
},K)
})
};
N.repeat=function(g,d){return null!=d?N.repeat(g).take(d):new N(function(){return new L(K,function(){return this.yieldReturn(g)
},K)
})
};
N.repeatWithFinalize=function(g,d){g=M.createLambda(g);
d=M.createLambda(d);
return new N(function(){var k;
return new L(function(){k=g()
},function(){return this.yieldReturn(k)
},function(){null!=k&&(d(k),k=null)
})
})
};
N.generate=function(g,d){if(null!=d){return N.generate(g).take(d)
}g=M.createLambda(g);
return new N(function(){return new L(K,function(){return this.yieldReturn(g())
},K)
})
};
N.toInfinity=function(g,d){null==g&&(g=0);
null==d&&(d=1);
return new N(function(){var k;
return new L(function(){k=g-d
},function(){return this.yieldReturn(k+=d)
},K)
})
};
N.toNegativeInfinity=function(g,d){null==g&&(g=0);
null==d&&(d=1);
return new N(function(){var k;
return new L(function(){k=g+d
},function(){return this.yieldReturn(k-=d)
},K)
})
};
N.unfold=function(g,d){d=M.createLambda(d);
return new N(function(){var m=!0,k;
return new L(K,function(){if(m){return m=!1,k=g,this.yieldReturn(k)
}k=d(k);
return this.yieldReturn(k)
},K)
})
};
N.defer=function(d){return new N(function(){var g;
return new L(function(){g=N.from(d()).getEnumerator()
},function(){return g.moveNext()?this.yieldReturn(g.current()):this.yieldBreak()
},function(){M.dispose(g)
})
})
};
N.prototype.traverseBreadthFirst=function(g,d){var k=this;
g=M.createLambda(g);
d=M.createLambda(d);
return new N(function(){var r,p=0,m=[];
return new L(function(){r=k.getEnumerator()
},function(){for(;
;
){if(r.moveNext()){return m.push(r.current()),this.yieldReturn(d(r.current(),p))
}var s=N.from(m).selectMany(function(t){return g(t)
});
