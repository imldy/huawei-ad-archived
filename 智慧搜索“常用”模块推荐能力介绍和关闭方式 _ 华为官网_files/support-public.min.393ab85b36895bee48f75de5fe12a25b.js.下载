}if(m!==r){return m=r,this.yieldReturn(p.current())
}}return this.yieldBreak()
},function(){M.dispose(p)
})
})
};
N.prototype.except=function(g,d){d=M.createLambda(d);
var k=this;
return new N(function(){var p,m;
return new L(function(){p=k.getEnumerator();
m=new h(d);
N.from(g).forEach(function(r){m.add(r)
})
},function(){for(;
p.moveNext();
){var r=p.current();
if(!m.contains(r)){return m.add(r),this.yieldReturn(r)
}}return !1
},function(){M.dispose(p)
})
})
};
N.prototype.intersect=function(g,d){d=M.createLambda(d);
var k=this;
return new N(function(){var r,p,m;
return new L(function(){r=k.getEnumerator();
