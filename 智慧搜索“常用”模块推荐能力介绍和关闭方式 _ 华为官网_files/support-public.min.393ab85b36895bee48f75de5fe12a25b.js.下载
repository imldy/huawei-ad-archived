var CitySelect=(function(){function b(g,f,d){this.__jqSelectorProvince__=g;
this.__jqSelectorCity__=f;
this.__jqSelectorArea__=d
}b.prototype.Bind=function(j){var i=this.__jqSelectorProvince__;
var g=this.__jqSelectorCity__;
var f=this.__jqSelectorArea__;
var d="";
var h="";
if(window.digitalData.page.pageInfo.uri.indexOf("/support/express-repair/create/")!=-1){d="pickup service";
h="pickup_service_personal_info_click_input_city"
}else{if(window.digitalData.page.pageInfo.uri.indexOf("/support/reservation/create/")!=-1||window.digitalData.page.pageInfo.uri.indexOf("/support/repair-appointment/create/")!=-1){d="repair reservation";
h="repair_reservation_personal_info_click_nput_city"
}}apiIData.queryRegionList({lang:sysConfig.lang,scopeGrade:"province",curpage:1,pagesize:100,parent_alpha_2_code:sysConfig.countryCode,sortField:"multi_lang_name"},function(m){if(m.responseCode=="200"&&m.responseData.list!=null&&m.responseData.list.length>0){$(i+" ol").empty();
for(var o=0;
o<m.responseData.list.length;
o++){var p=m.responseData.list[o];
$(i+" ol").append("<li onclick='sendDmpaByCurrentTag(this);gaAndAaByCurrentTag(this);haDispachByTag(this)' cat='"+d+"' ha-id='"+h+"' act='Click on fill personal info' lab='"+p.multi_lang_name+"' data-alpha_2_code='"+p.alpha_2_code+"' >"+p.multi_lang_name+"</li>")
}var q=i+" ol li:first-child";
var r=$("#contactProvinceSelect").attr("data-empty");
var l=$("#contactCitySelect").attr("data-empty");
var k=$("#contactAreaSelect").attr("data-empty");
var n=$("#contactStreetSelect").attr("data-empty");
$(i+" .select > span").attr("data-alpha_2_code","").empty().append(r);
$("#contactCitySelect .select > span").empty().append(l);
$("#contactAreaSelect .select > span").empty().append(k);
$("#contactStreetSelect .select > span").empty().append(n)
}$(i+" li").bind("click",function(){var s=$(this).attr("data-alpha_2_code");
clearReleatedAddressDatas(0);
